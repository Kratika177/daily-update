#include <iostream>
using namespace std;
int main()
{
    int num1;
    int num2;
    int num3;
    int sum=0;
  
   cin>>num1>>num2>>num3;

   cin>>num3;
   if((1<=num1&&num1<=1000) && (1<=num2&&num2<=1000) && (1<=num3&&num3<=1000) )
   {
       sum=num1+num2+num3;
       cout<<sum;
   }
   else {
       cout<<"Numbers should be in range 1 to 1000";
   }
}
